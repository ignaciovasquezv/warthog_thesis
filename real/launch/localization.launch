<?xml version="1.0"?>
<launch>
	
  <node name="odom_gps" pkg="package_nacho" type="odom_gps_topic.py" output="screen">
  </node>
  
  <node pkg="robot_localization" type="ekf_localization_node" name="ekf_localization">
    <rosparam command="load" file="$(find package_nacho)/config/ekf_config.yaml" />
    <!--remap from="/odometry/filtered" to="/new_odom"/-->
  </node>
  
  <node name="tf_odom" pkg="package_nacho" type="tf_odom.py" output="screen">
  </node>
</launch>
